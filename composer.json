{
    "name": "dahiorus/coloc-matching",
    "description": "A simple REST API for a house sharing application and administration application",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/yaml": "^4.0",
        "symfony/form": "^4.0",
        "symfony/asset": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/debug-bundle": "^4.0",
        "symfony/web-profiler-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/monolog-bundle": "3.1.*",
        "symfony/web-server-bundle": "^4.0",
        "symfony/proxy-manager-bridge": "3.4.*",
        "symfony/event-dispatcher": "3.4.*",
        "symfony/polyfill-apcu": "^1.0",
        "doctrine/orm": "2.*",
        "doctrine/doctrine-bundle": "1.*",
        "doctrine/doctrine-cache-bundle": "1.*",
        "sensio/framework-extra-bundle": "^3.0",
        "friendsofsymfony/rest-bundle": "2.3.*",
        "jms/serializer-bundle": "2.3.*",
        "lexik/jwt-authentication-bundle": "2.4.*",
        "egulias/email-validator": "2.1.*",
        "php-http/guzzle6-adapter": "1.1.*",
        "php-http/message": "1.6.*",
        "geocoder-php/google-maps-provider": "4.2.*",
        "ocramius/proxy-manager": "^2.1.1",
        "twig/extensions": "1.4.1",
        "nelmio/cors-bundle": "1.5.*",
        "willdurand/hateoas-bundle": "@stable",
        "nelmio/api-doc-bundle": "@stable",
        "facebook/graph-sdk": "^5.6",
        "google/apiclient": "^2.2"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "doctrine/doctrine-fixtures-bundle": "2.4.*",
        "phpunit/phpunit": "6.4.*",
        "guzzlehttp/guzzle": "6.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "authors": [
        {
            "name": "Brondon UNG",
            "email": "brondon.ung@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
