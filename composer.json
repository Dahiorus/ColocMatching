{
    "name": "dahiorus/coloc-matching",
    "description": "A simple REST API for a house sharing application",
    "license": "Apache-2.0",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "1.*",
        "doctrine/doctrine-cache-bundle": "1.*",
        "doctrine/orm": "^2.6",
        "egulias/email-validator": "2.1.*",
        "facebook/graph-sdk": "^5.6",
        "friendsofsymfony/rest-bundle": "^2.3",
        "geocoder-php/google-maps-provider": "^4.2",
        "google/apiclient": "^2.2",
        "jms/serializer-bundle": "^2.4",
        "lexik/jwt-authentication-bundle": "^2.5",
        "nelmio/api-doc-bundle": "^3.2",
        "nelmio/cors-bundle": "^1.5",
        "ocramius/proxy-manager": "^2.1.1",
        "php-http/guzzle6-adapter": "1.1.*",
        "php-http/message": "1.6.*",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.3",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/proxy-manager-bridge": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0",
        "twig/extensions": "^1.5",
        "willdurand/hateoas-bundle": "^1.4"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0",
        "symfony/browser-kit": "^4.0",
        "symfony/debug-bundle": "^4.0",
        "symfony/web-server-bundle": "^4.0",
        "symfony/web-profiler-bundle": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "doctrine/doctrine-fixtures-bundle": "^2.4",
        "phpunit/phpunit": "^7.3",
        "guzzlehttp/guzzle": "^6.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "authors": [
        {
            "name": "Brondon UNG",
            "email": "brondon.ung@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
