{
    "name": "brondon-ung/coloc-matching",
    "description": "A simple REST API for a house sharing application and administration application",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "3.4.*",
        "doctrine/orm": "2.*",
        "doctrine/doctrine-bundle": "1.*",
        "doctrine/doctrine-cache-bundle": "1.*",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "3.1.*",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "incenteev/composer-parameter-handler": "2.1.3",
        "friendsofsymfony/rest-bundle": "2.3.*",
        "jms/serializer-bundle": "2.3.*",
        "lexik/jwt-authentication-bundle": "2.4.*",
        "egulias/email-validator": "2.1.*",
        "php-http/guzzle6-adapter": "1.1.*",
        "php-http/message": "1.6.*",
        "geocoder-php/google-maps-provider": "4.1.*",
        "symfony/assetic-bundle": "2.8.*",
        "ocramius/proxy-manager": "^2.1.1",
        "symfony/proxy-manager-bridge": "3.4.*",
        "symfony/event-dispatcher": "3.4.*",
        "twig/extensions": "1.4.1",
        "nelmio/cors-bundle": "1.5.*",
        "willdurand/hateoas-bundle": "@stable",
        "zircote/swagger-php": "2.0.*"
    },
    "require-dev": {
        "sensio/generator-bundle": "3.1.*",
        "symfony/phpunit-bridge": "3.3.*",
        "doctrine/doctrine-fixtures-bundle": "2.4.*",
        "phpunit/phpunit": "6.4.*",
        "guzzlehttp/guzzle": "6.3.*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "authors": [
        {
            "name": "Brondon UNG",
            "email": "brondon.ung@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
