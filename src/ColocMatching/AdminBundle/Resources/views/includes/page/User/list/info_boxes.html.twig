<div class="row">
	{# Proposal info box #}
	<div class="col-md-6 col-sm-6 col-xs-12">
		<div class="info-box">
			<span class="info-box-icon bg-yellow"><i class="fa fa-user"></i></span>
			
			<div class="info-box-content">
				<span class="info-box-text">{{ "label.info.users.proposal"|trans }}</span>
				<span class="info-box-number">{{ proposalCount }}</span>
			</div>
		</div>
	</div>
	
	{# Search info box #}
	<div class="col-md-6 col-sm-6 col-xs-12">
		<div class="info-box">
			<span class="info-box-icon bg-yellow"><i class="fa fa-user"></i></span>
			
			<div class="info-box-content">
				<span class="info-box-text">{{ "label.info.users.search"|trans }}</span>
				<span class="info-box-number">{{ searchCount }}</span>
			</div>
		</div>
	</div>
</div>
