{% extends "::base.html.twig" %}

{% block title %}{{- "title.administration"|trans -}}{% endblock title %}


{% block stylesheets %}
	{% stylesheets filter="cssrewrite"
	    "css/bootstrap.min.css"
	    "plugins/*/css/*.css"
	    "plugins/*/*.css"
		"css/*.css"
	    "css/skins/*.css" %}
	<link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}
	
	{% javascripts
		"js/jquery-3.2.1.min.js"
		"js/bootstrap.js"
		"js/app.js" %}
	<script src="{{ asset_url }}" charset="UTF-8"></script>
	{% endjavascripts %}
{% endblock stylesheets %}


{% block javascripts %}
	{% javascripts
		"plugins/*/*.js"
	    "plugins/*/*/*.js" %}
	<script src="{{ asset_url }}" charset="UTF-8"></script>
	{% endjavascripts %}
{% endblock javascripts %}

