{% extends "::base.html.twig" %}

{% block title %}{{- "title.administration"|trans -}}{% endblock title %}


{% block stylesheets %}
	{% stylesheets filter="cssrewrite"
	    "css/bootstrap.min.css"
	    "plugins/*/css/*.css"
	    "plugins/*/*.css"
		"css/*.css"
	    "css/skins/*.css" %}
	<link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}
{% endblock stylesheets %}


{% block javascripts %}
	{% javascripts
		"js/jquery-3.2.1.min.js"
		"js/bootstrap.js"
		"js/*.js"
		"plugins/*/*.js" %}
	<script src="{{ asset_url }}"></script>
	{% endjavascripts %}
{% endblock javascripts %}

