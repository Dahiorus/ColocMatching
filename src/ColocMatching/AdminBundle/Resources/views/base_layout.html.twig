{% extends "::base.html.twig" %}

{% block title %}{{- "title.administration"|trans -}}{% endblock title %}


{% block stylesheets %}
	{% stylesheets filter="cssrewrite"
		"@AdminBundle/Resources/public/css/*.css"
	    "@AdminBundle/Resources/public/plugins/*/*.css" %}
	<link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}
{% endblock stylesheets %}


{% block javascripts %}
	{% javascripts
		"@AdminBundle/Resources/public/plugins/*/*.js"
	    "@AdminBundle/Resources/public/js/*.js" %}
	<script src="{{ asset_url }}"></script>
	{% endjavascripts %}
{% endblock javascripts %}

